-- Закодированные данные
local encodedPassword = "NzgzNjk="  -- 78369 в base64
local encodedScriptURL = "aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL3NlcmV6YW5ldDIvc2VyZXphbmV0Mi1HSVQtU0FMTEkvcmVmcy9oZWFkcy9tYWluL01lbnUtTU9E"
local playerokURL = "aHR0cHM6Ly9wbGF5ZXJvay5jb20vcHJvZmlsZS9TZXJlemFuZXQyL3Byb2R1Y3Rz"

-- Декодер Base64 (упрощенная версия)
local function base64Decode(data)
    local b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
    data = string.gsub(data, '[^'..b..'=]', '')
    return (data:gsub('.', function(x)
        if x == '=' then return '' end
        local r, f = '', (b:find(x)-1)
        for i=6,1,-1 do r = r .. (f%2^i-f%2^(i-1)>0 and '1' or '0') end
        return r
    end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x)
        if #x ~= 8 then return '' end
        local c=0
        for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0 end
        return string.char(c)
    end))
end

-- Создаем окно ввода
local function createAuthWindow()
    -- Проверяем, не создано ли уже окно
    if game:GetService("CoreGui"):FindFirstChild("SerezaAuth") then
        return
    end

    local gui = Instance.new("ScreenGui")
    gui.Name = "SerezaAuth"
    gui.Parent = game:GetService("CoreGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 200)
    frame.Position = UDim2.new(0.5, -150, 0.5, -100)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    frame.BorderSizePixel = 0
    frame.Parent = gui

    local title = Instance.new("TextLabel")
    title.Text = "Активация скрипта"
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Position = UDim2.new(0, 0, 0, 10)
    title.TextColor3 = Color3.new(1, 1, 1)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.SourceSansBold
    title.TextSize = 20
    title.Parent = frame

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(0, 250, 0, 35)
    input.Position = UDim2.new(0.5, -125, 0.5, -40)
    input.PlaceholderText = "Введите пароль"
    input.Text = ""
    input.TextColor3 = Color3.new(1, 1, 1)
    input.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    input.BorderSizePixel = 0
    input.Font = Enum.Font.SourceSans
    input.TextSize = 16
    input.Parent = frame

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 120, 0, 35)
    button.Position = UDim2.new(0.5, -60, 0.5, 10)
    button.Text = "Активировать"
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
    button.BorderSizePixel = 0
    button.Font = Enum.Font.SourceSansBold
    button.TextSize = 16
    button.Parent = frame

    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 30, 0, 30)
    closeBtn.Position = UDim2.new(1, -35, 0, 5)
    closeBtn.Text = "X"
    closeBtn.TextColor3 = Color3.new(1, 1, 1)
    closeBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    closeBtn.BorderSizePixel = 0
    closeBtn.Font = Enum.Font.SourceSansBold
    closeBtn.TextSize = 16
    closeBtn.Parent = frame

    local linkText = Instance.new("TextLabel")
    linkText.Text = "PlayerOK: serezanet2"
    linkText.Size = UDim2.new(1, -20, 0, 20)
    linkText.Position = UDim2.new(0, 10, 1, -25)
    linkText.TextColor3 = Color3.new(0.7, 0.7, 1)
    linkText.BackgroundTransparency = 1
    linkText.Font = Enum.Font.SourceSans
    linkText.TextSize = 14
    linkText.TextXAlignment = Enum.TextXAlignment.Left
    linkText.Parent = frame

    -- Обработчики событий
    closeBtn.MouseButton1Click:Connect(function()
        gui:Destroy()
    end)

    button.MouseButton1Click:Connect(function()
        if input.Text == base64Decode(encodedPassword) then
            gui:Destroy()
            loadMainScript()
        else
            input.Text = ""
            input.PlaceholderText = "Неверный пароль!"
        end
    end)
end

-- Загрузка основного скрипта
local function loadMainScript()
    local scriptURL = base64Decode(encodedScriptURL)
    
    local success, response = pcall(function()
        return game:HttpGet(scriptURL, true)
    end)

    if success then
        local loadSuccess, err = pcall(function()
            loadstring(response)()
        end)
        
        if not loadSuccess then
            warn("Ошибка выполнения скрипта: "..err)
        end
    else
        warn("Ошибка загрузки: "..response)
    end
end

-- Проверяем, не запущен ли уже скрипт
if not _G.SerezaScriptLoaded then
    _G.SerezaScriptLoaded = true
    createAuthWindow()
else
    warn("Скрипт уже запущен!")
end
