--[[
  üîê –°–∏—Å—Ç–µ–º–∞ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ —Å –∫–æ–¥–∏—Ä–æ–≤–∞–Ω–Ω—ã–º –∫–ª—é—á–æ–º
  1. –ö–ª—é—á "78369" –∑–∞–∫–æ–¥–∏—Ä–æ–≤–∞–Ω –≤ base64
  2. –û—Å–Ω–æ–≤–Ω–æ–π —Å–∫—Ä–∏–ø—Ç –∑–∞–∫–æ–¥–∏—Ä–æ–≤–∞–Ω –∏ –ø—Ä–æ–≤–µ—Ä—è–µ—Ç—Å—è
  3. –ó–∞–ø—É—Å–∫–∞–µ—Ç—Å—è —Ç–æ–ª—å–∫–æ –ø–æ—Å–ª–µ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–≥–æ –≤–≤–æ–¥–∞
]]

-- –ó–∞–∫–æ–¥–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ
local encodedKey = "NzgzNjk=" -- 78369 –≤ base64
local encodedScript = "bG9hZHN0cmluZyhnYW1lOkdldEh0dHAoJ2h0dHBzOi8vcmF3LmdpdGh1YnVzZXJjb250ZW50LmNvbS9zZXJlemFuZXQyL3NlcmV6YW5ldDItR0lULVNBTExJL3JlZnMvaGVhZHMvbWFpbi9zY3JpcHQta2V5JykpKCk="

-- –î–µ–∫–æ–¥–µ—Ä Base64
local function base64Decode(data)
    local b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
    data = string.gsub(data, '[^'..b..'=]', '')
    return (data:gsub('.', function(x)
        if x == '=' then return '' end
        local r, f = '', (b:find(x)-1)
        for i=6,1,-1 do r = r .. (f%2^i-f%2^(i-1)>0 and '1' or '0') end
        return r
    end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x)
        if #x ~= 8 then return '' end
        local c=0
        for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end
        return string.char(c)
    end))
end

-- –°–æ–∑–¥–∞–µ–º –æ–∫–Ω–æ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏
local function createAuthWindow()
    local gui = Instance.new("ScreenGui")
    gui.Name = "KeyAuth"
    gui.Parent = game:GetService("CoreGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 180)
    frame.Position = UDim2.new(0.5, -150, 0.5, -90)
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    frame.BorderSizePixel = 0
    frame.Parent = gui

    local title = Instance.new("TextLabel")
    title.Text = "üîê –ê–∫—Ç–∏–≤–∞—Ü–∏—è —Å–∫—Ä–∏–ø—Ç–∞"
    title.Size = UDim2.new(1, 0, 0, 40)
    title.Position = UDim2.new(0, 0, 0, 10)
    title.TextColor3 = Color3.new(1, 1, 1)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.SourceSansBold
    title.TextSize = 18
    title.Parent = frame

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(0, 250, 0, 35)
    input.Position = UDim2.new(0.5, -125, 0.5, -30)
    input.PlaceholderText = "–í–≤–µ–¥–∏—Ç–µ –∫–ª—é—á –∞–∫—Ç–∏–≤–∞—Ü–∏–∏"
    input.Text = ""
    input.TextColor3 = Color3.new(1, 1, 1)
    input.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    input.BorderSizePixel = 0
    input.Font = Enum.Font.SourceSans
    input.TextSize = 16
    input.Parent = frame

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 120, 0, 35)
    button.Position = UDim2.new(0.5, -60, 0.7, 0)
    button.Text = "–ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞—Ç—å"
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    button.BorderSizePixel = 0
    button.Font = Enum.Font.SourceSansBold
    button.TextSize = 16
    button.Parent = frame

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞
    button.MouseButton1Click:Connect(function()
        if input.Text == base64Decode(encodedKey) then
            gui:Destroy()
            loadMainScript()
        else
            input.Text = ""
            input.PlaceholderText = "‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –∫–ª—é—á!"
        end
    end)
end

-- –ó–∞–≥—Ä—É–∑–∫–∞ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ —Å–∫—Ä–∏–ø—Ç–∞
local function loadMainScript()
    local success, decoded = pcall(base64Decode, encodedScript)
    if not success then
        warn("[–û—à–∏–±–∫–∞] –î–µ–∫–æ–¥–∏—Ä–æ–≤–∞–Ω–∏–µ —Å–∫—Ä–∏–ø—Ç–∞ –Ω–µ —É–¥–∞–ª–æ—Å—å!")
        return
    end

    local loadSuccess, err = pcall(loadstring(decoded))
    if not loadSuccess then
        warn("[–û—à–∏–±–∫–∞] –ó–∞–ø—É—Å–∫ —Å–∫—Ä–∏–ø—Ç–∞: "..err)
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∏—Å—Ç–µ–º—É
if not _G.SerezaAuthLoaded then
    _G.SerezaAuthLoaded = true
    createAuthWindow()
end
